{
    "id": "javascript",
    "patterns": ["function_expression"],
    "features": {
        "punctuation": {
            "match": "\\W",
            "id": "punctuation" 
        },
        "whitespace": {
            "match": "\\s",
            "id": ""
        },
        "function": {
            "match": "\\b[a-z][\\w]*?(?=\\()\\b",
            "id": "function"
        },
        "paren_begin": {
            "match": "\\(",
            "id": "paren.open"
        },
        "paren_end": {
            "match": "\\)",
            "id": "paren.close"
        },
        "bracket_begin": {
            "match": "\\{",
            "id": "bracket.open"
        },
        "bracket_end": {
            "match": "\\}",
            "id": "bracket.close"
        },
        "keyword": {
            "match": "\\b(abstract|break|char|debugger|double|export|finally|goto|in|let|null|public|super|throw|try|volatile|arguments|byte|class|default|else|extends|float|if|instanceof|long|package|return|switch|throws|typeof|while|await|case|const|delete|enum|false|for|implements|int|native|private|short|synchronized|transient|var|with|boolean|catch|continue|do|eval|final|function|import|interface|new|protected|static|this|true|void|yield|from)\\b",
            "id": "keyword"
        },
        "number": {
            "match": "\\b\\d+?\\b",
            "id": "number"
        },
        "variable": {
            "match": "\\b[a-z]([\\w]+)?",
            "id": "variable"
        },
        "object": {
            "match": "\\b[A-Z]([\\w]+)?",
            "id": "object"
        },
        "operator": {
            "match": "\\=|\\+|\\-|\\*|\\/|\\%|\\+\\+|\\-\\\\\\\\-|\\+\\=|\\-\\=|\\*\\=|\\/\\=|\\%\\=|\\=\\=|\\=\\=\\=|\\!\\=|\\!\\=\\=|\\>|\\<|\\>\\=|\\<\\=|\\?|\\:|\\&\\&|\\|\\||\\!|\\&|\\||\\~|\\^|\\<\\<|\\>\\>",
            "id": "operator"
        },
        "newline": {
            "match": "\\n",
            "id": "newline"
        },
        "string_template": {
            "match": "\".+?\"",
            "id": "string-template"
        },
        "string_literal": {
            "match": "\\'.+?\\'",
            "id": "string-literal"
        },
        "template_literal": {
            "match": "\\`.+?\\`",
            "id": "template-literal"
        },
        "line_comment": {
            "match": "\/\/.+",
            "id": "line-comment"
        },
        "multiline_comment": {
            "match": "\/\\*.+",
            "id": "multiline-comment",
            "multi": {
                "start": {
                    "match": "\/\\*",
                    "id": "multiline-comment-start"
                },
                "end": {
                    "match": "\\*\/",
                    "id": "multiline-comment-end"
                }
            }
        },
        "multiline_comment_line": {
            "match": "\/\\*.+?\\*\/",
            "id": "multiline-comment-line"
        }
    },
    "keywords": [
        "abstract",
        "break",
        "char",
        "debugger",
        "double",
        "export",
        "finally",
        "goto",
        "in",
        "let",
        "null",
        "public",
        "super",
        "throw",
        "try",
        "volatile",
        "arguments",
        "byte",
        "class",
        "default",
        "else",
        "extends",
        "float",
        "if",
        "instanceof",
        "long",
        "package",
        "return",
        "switch",
        "throws",
        "typeof",
        "while",
        "await",
        "case",
        "const",
        "delete",
        "enum",
        "false",
        "for",
        "implements",
        "int",
        "native",
        "private",
        "short",
        "synchronized",
        "transient",
        "var",
        "with",
        "boolean",
        "catch",
        "continue",
        "do",
        "eval",
        "final",
        "function",
        "import",
        "interface",
        "new",
        "protected",
        "static",
        "this",
        "true",
        "void",
        "yield",
        "from"
    ],
    "operators": [
        "=",
        "+",
        "-",
        "*",
        "/",
        "%",
        "++",
        "-\\-",
        "+=",
        "-=",
        "*=",
        "/=",
        "%=",
        "==",
        "===",
        "!=",
        "!==",
        ">",
        "<",
        ">=",
        "<=",
        "?",
        ":",
        "&&",
        "||",
        "!",
        "&",
        "|",
        "~",
        "^",
        "<<",
        ">>"
    ],
    "string_template": "\"",
    "string_literal": "'",
    "line_comment": "\/\/",
    "multiline_comment": {
        "start": "\/\\*",
        "end": "\\*\/"
    },
    "docs_comment": {
        "start": "\/\\*\\*",
        "middle": "\\*",
        "end": "\\*\/"
    }
}